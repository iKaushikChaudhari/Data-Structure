#include<iostream>

using namespace std;

int main(){
    int a[50],key,n,flag=0;
    cout<<"Enter the value of key :";
    cin>>key;
    cout<<"Enter how much you want to iterate the loop :";
    cin>>n;
    for (int i = 0; i < n; i++)
    {
        cin>>a[i];
    }
    for (int i = 0; i < n; i++)
    {
        if (a[i]==key)
        {
            flag = 1
            break;
        }
    }
    if(flag == 1){
        cout<<"The Number is found inside the array";
    }
    else{
        cout<<"The Number which is key is not present inside the array";
    }
    
    
    return 0;
}
